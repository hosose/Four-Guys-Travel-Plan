<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.kosta.fourguys.mapper.MemberMapper">
	<insert id="registerMember" parameterType="org.kosta.fourguys.vo.MemberVO">
		insert into member values(#{id},#{password},#{name},#{address},#{birth},#{email},#{phone})
	</insert>
	<update id="updateMember" parameterType="org.kosta.fourguys.vo.MemberVO">
		update member set name = #{name} ,password=#{password},address=#{address},phone=#{phone} where id=#{id}
	</update>
	<delete id="deleteMember" parameterType="org.kosta.fourguys.vo.MemberVO">
		delete member where id = #{id}
	</delete>

</mapper>