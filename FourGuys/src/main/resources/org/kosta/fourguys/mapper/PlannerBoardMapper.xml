<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.kosta.fourguys.mapper.PlannerBoardMapper">
<select id="findPlannerBoardByNo" parameterType="org.kosta.fourguys.vo.PlannerBoardVO">
 SELECT * FROM planner_board WHERE board_no = #{board_no}
</select>
<update id="editBoardById" parameterType="org.kosta.fourguys.vo.PlannerBoardVO">
update planner_board
set board_title= #{boardTitle}, board_content = #{boardContent}
where id= #{id}
</update>
</mapper>